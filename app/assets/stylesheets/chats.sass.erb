#streams_wrapper
  
  
  
  +outer-container
  height: 500px
  
  #streams_container
  
    height: 300px
    
    +span-columns(9)
    padding: 5px
    
    #ovid1, #ovid2, #ovid3, #ovid4, #ovid5
      display: inline-block
      width: 155px
      height: 150px
        
      #ivid1, #ivid2, #ivid3, #ivid4, #ivid5
        width: 155px
        height: 150px
        background: url("man.png")
        background-size: 155px 150px
        background-repeat: no-repeat
        padding-bottom: 30px
        border: 1px solid
          
      
    
  #chats_aside
    +span-columns(3)
    background: gray
    border: 3px solid
    overflow: hidden
    word-wrap: break-word
#streams_bin
  width: 808px
  height: 150px

#join_butt_1, #join_butt_2, #join_butt_3, #join_butt_4, #join_butt_5,
#quit_butt_1, #quit_butt_2, #quit_butt_3, #quit_butt_4, #quit_butt_5
  width: 155px
  height: 20px
  vertical-align: bottom
  padding-bottom: 30px  
  

  

.overlay
  background-color: rgba(255, 0, 0, 0.2)
  width: 155px
  height: 150px
  
.join_text
  font-size: .9em